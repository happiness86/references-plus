{
  "publisher": "happiness86",
  "name": "vscode-references-plus",
  "displayName": "References Plus",
  "version": "0.0.14",
  "private": false,
  "packageManager": "pnpm@8.10.5",
  "description": "",
  "author": "happiness86",
  "license": "MIT",
  "homepage": "https://github.com/happiness86/references-plus#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/happiness86/references-plus"
  },
  "bugs": {
    "url": "https://github.com/happiness86/references-plus/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "properties": {
        "references-plus.max": {
          "type": "integer",
          "default": 50,
          "description": "Maximum number of historical results"
        },
        "references-plus.preserveWhitespace": {
          "type": "boolean",
          "default": true,
          "description": "Whether to display blank space at the begining of leaf node"
        },
        "references-plus.silent": {
          "type": "boolean",
          "default": false,
          "description": "Whether to push notification when search again"
        },
        "references-plus.replenish": {
          "type": "boolean",
          "default": false,
          "description": "Whether to replenish the deleted data when search again"
        }
      }
    },
    "commands": [
      {
        "command": "references-plus.getAllReferences",
        "title": "References Plus"
      },
      {
        "command": "references-plus.refresh",
        "title": "refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "references-plus.selectNode",
        "title": "selectNode"
      },
      {
        "command": "references-plus.clear",
        "title": "clear",
        "icon": "$(clear-all)"
      },
      {
        "command": "references-plus.collapseAll",
        "title": "collapse all",
        "icon": "$(debug-collapse-all)"
      },
      {
        "command": "references-plus.deleteEntry",
        "title": "delete",
        "icon": "$(close)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "references-plus",
          "title": "References Plus",
          "icon": "res/icon.png"
        }
      ]
    },
    "views": {
      "references-plus": [
        {
          "id": "references-plus",
          "name": "Result"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "references-plus.getAllReferences"
        }
      ],
      "view/title": [
        {
          "command": "references-plus.refresh",
          "group": "navigation@1",
          "when": "view == references-plus"
        },
        {
          "command": "references-plus.clear",
          "group": "navigation@2",
          "when": "view == references-plus"
        }
      ],
      "view/item/context": [
        {
          "command": "references-plus.deleteEntry",
          "group": "inline",
          "when": "view == references-plus"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.83.0",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "pnpm": "^8.10.5",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
